generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  nome String
  nome_usuario String @unique
  email String @unique
  senha String
  cargo String
  admin Int

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt



  @@map("user")
}
